{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "VITE_API_BASE_URL": "@api_base_url",
    "VITE_AUTH_DOMAIN": "@auth_domain",
    "VITE_AUTH_CLIENT_ID": "@auth_client_id"
  }
}
