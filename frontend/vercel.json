{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://typeaware-backend.onrender.com",
    "VITE_AUTH_DOMAIN": "",
    "VITE_AUTH_CLIENT_ID": ""
  }
}
