{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_AUTH_DOMAIN": "",
    "VITE_AUTH_CLIENT_ID": "",
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://typeaware-cycberbully-ai.onrender.com/api/$1"
    }
  ],
  "routes": [
    {
      "src": "/assets/(?<path>.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$path"
    },
    {
      "src": "/index\\.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "dest": "/index.html"
    },
    {
      "src": "/(?!assets/).*",
      "dest": "/index.html"
    }
  ]
}